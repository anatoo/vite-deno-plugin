    "react-dom": "https://esm.sh/react-dom@18.2.0",
    "react-dom/": "https://esm.sh/react-dom@18.2.0/",
    "wouter": "https://esm.sh/wouter@2.12.0"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
